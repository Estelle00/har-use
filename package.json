{
  "name": "@har/use",
  "version": "2.2.5",
  "description": "",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite ./example --config vite.config.ts",
    "docs": "vitepress dev src",
    "build:types": "rimraf dist/types && tsc -p ./tsconfig.json --emitDeclarationOnly",
    "build:bundle": "har build:component",
    "build": "pnpm build:bundle && pnpm build:types",
    "release": "pnpm build && release-it",
    "prepare": "pnpm build"
  },
  "publishConfig": {
    "registry": "https://nexus.huianrong.com/repository/npm-hosted/"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.26.0",
    "@dcloudio/types": "^2.6.6",
    "@har/cli": "^1.0.2",
    "@har/eslint-config": "^1.1.1",
    "@har/vite-plugin-doc": "^1.0.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/markdown-it": "^12.2.3",
    "@types/prismjs": "^1.16.6",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.20",
    "change-case": "^4.1.2",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "less": "^4.1.2",
    "markdown-it": "^13.0.1",
    "prettier": "^2.5.1",
    "prismjs": "^1.25.0",
    "release-it": "^14.11.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "4.5.3",
    "vite": "^2.6.14",
    "vitepress": "^0.22.4",
    "vue": "^3.2.24"
  },
  "repository": {
    "type": "git",
    "url": "https://git.huianrong.com/frontend/har-use"
  },
  "author": "liubingwen",
  "license": "ISC",
  "release-it": {
    "git": {
      "tag": true,
      "commitMessage": "release: @har/use ${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "peerDependencies": {
    "vue": "^3.2"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
