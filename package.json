{
  "name": "@har/use",
  "packageManager": "pnpm@7.1.2",
  "scripts": {
    "build:tsc": "tsc -b tsconfig.build.json",
    "dev:tsc": "pnpm build:tsc --watch",
    "build:all": "pnpm -r --stream build",
    "clean": "pnpm --parallel --stream clean",
    "docs:build": "pnpm --filter=docs docs:build",
    "docs:dev": "pnpm --filter=docs docs:dev",
    "release:publish": "pnpm -r publish --tag beta"
  },
  "publishConfig": {
    "registry": "https://nexus.huianrong.com/repository/npm-hosted/"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@har/eslint-config": "^1.3.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "@types/node": "16.11.7",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://git.huianrong.com/frontend/har-use"
  },
  "author": "liubingwen"
}
